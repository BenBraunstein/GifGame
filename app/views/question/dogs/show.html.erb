<h1> Dog Breed Question <%= @question.id %> </h1>

<%= display_prompt("dog", @question).html_safe %>

<% randomize_choices("dog", @question).each do |choice|%>

    <li><%= label_tag display_choice("dog", choice) %>
        <%= "(correct)" if choice == @question.a %>
    </li>

<% end %>


<%= button_to "Delete", question_dog_path(@question), method: :delete%>

